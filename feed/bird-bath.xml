<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<generator uri="https://jekyllrb.com/" version="4.3.2">Jekyll</generator>
	<link href="{{ site.url }}/feed/bird-bath.xml" rel="self" type="application/atom+xml"/>
	<link href="{{ site.url }}" rel="alternate" type="text/html"/>
	<updated>2023-08-19T00:32:42-04:00</updated>
	<id>{{ site.url }}/feed/bird-bath.xml</id>
	<title type="text">{{ site.title }} | Bird Bath</title>
	<subtitle/>
	<author>
		<name>{{ site.author }}</name>
	</author>
	{% for post in site.categories.bird-bath %}
		{% if post.link %}
		<entry>
			<title type="text">{{ post.title | xml_escape }}</title>
			<link href="{{ post.link | escape }}" rel="alternate" type="text" title="{{ post.title | xml_escape }}"/>
			<published>{{ post.date | date_to_rfc822 }}</published>
			<updated>{{ post.date | date_to_rfc822 }}</updated>
			<id>http://localhost:4000/2023/08/14/external</id>
			<author>
				<name>{{ site.author }}</name>
			</author>
			<category term="bird-bath"/>
		</entry>
		{% endif %}
	{% endfor %}
</feed>